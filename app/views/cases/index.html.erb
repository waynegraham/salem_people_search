<h1>Listing cases</h1>

<table>
  <thead>
    <tr>
      <th>Case</th>
      <th>Doc</th>
      <th>Type</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cases.each do |c| %>
      <tr>
        <td><%= c.case_id %></td>
        <td><%= c.doc_id %></td>
        <td><%= c.doc_type %></td>
        <td><%= c.content %></td>
      </tr>
    <% end %>
  </tbody>
</table>


